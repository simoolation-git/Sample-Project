@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Locked Out";
    Layout = "~/Views/Shared/_LayoutWithFooter.cshtml";
}

<div class="row min-height">
    <div class="col-xs-10 col-md-6 col-lg-5 col-xl-4 center-block float-none">
        <div class="text-danger msg-header">@ViewBag.Title</div>
        <div class="text-danger msg-text">This account has been locked out, please try again later.</div>
    </div>
</div>